<?xml version="1.0" encoding="utf-8"?>
<com.lynn.simplerecyclerview.drag.viewdraghelper.NSScrollView
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white">

    <com.lynn.simplerecyclerview.drag.viewdraghelper.DragContainer
        xmlns:android="http://schemas.android.com/apk/res/android"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <android.support.v7.widget.AppCompatTextView
            android:id="@+id/a"
            android:layout_width="match_parent"
            android:layout_height="@dimen/dimen_50"
            android:layout_marginLeft="@dimen/dimen_25"
            android:layout_marginRight="@dimen/dimen_25"
            android:background="@color/color_4A4A4A"
            android:gravity="center"
            android:text="AAA"
            android:textColor="@color/white"/>

        <android.support.v7.widget.AppCompatTextView
            android:id="@+id/b"
            android:layout_width="match_parent"
            android:layout_height="@dimen/dimen_100"
            android:layout_below="@+id/a"
            android:layout_marginLeft="@dimen/dimen_25"
            android:layout_marginRight="@dimen/dimen_25"
            android:background="@color/color_FF3049"
            android:gravity="center"
            android:text="BBB"
            android:textColor="@color/white"/>

        <android.support.v7.widget.AppCompatTextView
            android:id="@+id/c"
            android:layout_width="match_parent"
            android:layout_height="@dimen/dimen_150"
            android:layout_below="@+id/b"
            android:layout_marginLeft="@dimen/dimen_25"
            android:layout_marginRight="@dimen/dimen_25"
            android:background="@color/colorPrimary"
            android:gravity="center"
            android:text="CCC"
            android:textColor="@color/white"/>

        <android.support.v7.widget.AppCompatTextView
            android:id="@+id/d"
            android:layout_width="match_parent"
            android:layout_height="@dimen/dimen_200"
            android:layout_below="@+id/c"
            android:layout_marginLeft="@dimen/dimen_25"
            android:layout_marginRight="@dimen/dimen_25"
            android:background="@color/color_ECF0F1"
            android:gravity="center"
            android:text="DDD"
            android:textColor="@color/color_333333"/>

        <android.support.v7.widget.AppCompatTextView
            android:id="@+id/e"
            android:layout_width="match_parent"
            android:layout_height="@dimen/dimen_280"
            android:layout_below="@+id/d"
            android:layout_marginLeft="@dimen/dimen_25"
            android:layout_marginRight="@dimen/dimen_25"
            android:background="@color/color_54CB63"
            android:gravity="center"
            android:text="EEE"
            android:textColor="@color/white"/>

        <android.support.v7.widget.AppCompatTextView
            android:id="@+id/f"
            android:layout_width="match_parent"
            android:layout_height="@dimen/dimen_280"
            android:layout_below="@+id/e"
            android:layout_marginLeft="@dimen/dimen_25"
            android:layout_marginRight="@dimen/dimen_25"
            android:background="@color/color_00D99D"
            android:gravity="center"
            android:text="FFF"
            android:textColor="@color/white"/>

        <android.support.v7.widget.AppCompatTextView
            android:id="@+id/g"
            android:layout_width="match_parent"
            android:layout_height="@dimen/dimen_280"
            android:layout_below="@+id/f"
            android:layout_marginLeft="@dimen/dimen_25"
            android:layout_marginRight="@dimen/dimen_25"
            android:background="@color/color_3A7BF7"
            android:gravity="center"
            android:text="GGG"
            android:textColor="@color/white"/>

        <android.support.v7.widget.AppCompatTextView
            android:id="@+id/h"
            android:layout_width="match_parent"
            android:layout_height="@dimen/dimen_280"
            android:layout_below="@+id/g"
            android:layout_marginLeft="@dimen/dimen_25"
            android:layout_marginRight="@dimen/dimen_25"
            android:background="@color/color_4A4A4A"
            android:gravity="center"
            android:text="HHH"
            android:textColor="@color/white"/>

        <android.support.v7.widget.AppCompatTextView
            android:id="@+id/i"
            android:layout_width="match_parent"
            android:layout_height="@dimen/dimen_280"
            android:layout_below="@+id/h"
            android:layout_marginLeft="@dimen/dimen_25"
            android:layout_marginRight="@dimen/dimen_25"
            android:background="@color/color_4F84FA"
            android:gravity="center"
            android:text="III"
            android:textColor="@color/white"/>

        <android.support.v7.widget.AppCompatTextView
            android:id="@+id/j"
            android:layout_width="match_parent"
            android:layout_height="@dimen/dimen_280"
            android:layout_below="@+id/i"
            android:layout_marginLeft="@dimen/dimen_25"
            android:layout_marginRight="@dimen/dimen_25"
            android:background="@color/color_9B9B9B"
            android:gravity="center"
            android:text="JJJ"
            android:textColor="@color/white"/>

        <android.support.v7.widget.AppCompatTextView
            android:id="@+id/k"
            android:layout_width="match_parent"
            android:layout_height="@dimen/dimen_280"
            android:layout_below="@+id/j"
            android:layout_marginLeft="@dimen/dimen_25"
            android:layout_marginRight="@dimen/dimen_25"
            android:background="@color/color_54CB63"
            android:gravity="center"
            android:text="KKK"
            android:textColor="@color/white"/>

    </com.lynn.simplerecyclerview.drag.viewdraghelper.DragContainer>
</com.lynn.simplerecyclerview.drag.viewdraghelper.NSScrollView>